{
    "background": {
        "service_worker": "background.js"
    },
    "author": "Nick Semenkovich",
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'"
    },
    "description": "Set a maximum limit on history entry lifetime. By Nick Semenkovich <semenko@alum.mit.edu>",
    "manifest_version": 3,
    "name": "Limit History Lifetime",
    "options_ui": {
        "page": "options.html",
        "open_in_tab": false
    },
    "permissions": [
        "alarms",
        "history",
        "storage"
    ],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "version": "2014.7.20"
}